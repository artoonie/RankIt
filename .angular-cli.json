      "root": "modules/core/src",
      "outDir": "modules/core/dist-demo",
      "assets": [
      ],
      "index": "demo/index.html",
      "main": "demo/main.ts",
      "polyfills": "demo/polyfills.ts",
      "test": "lib/test.ts",
      "tsconfig": "demo/tsconfig.json",
      "testTsconfig": "lib/tsconfig.spec.json",
      "prefix": "rankit",
      "styles": [],
      "scripts": [],
      "environmentSource": "demo/environments/environment.ts",
      "environments": {
        "dev": "demo/environments/environment.ts",
        "prod": "demo/environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "modules/core/demo/tsconfig.app.json"
    },
    {
      "project": "modules/core/lib/tsconfig.spec.json"
