  "scripts": {
    "ng": "ng",
    "start": "concurrently \"npm run start:widget-browser\" \"npm run start:widget-server\"",
    "start:widget:browser": "ng serve --app widget-browser --port 4400 ",
    "start:widget:server": "concurrently \"cd modules/widget && webpack --watch \" \"lite-server -c bs.server-widget.config.json\"",
    "start:website": "ng serve --app website --port 4404",
    "dev": "concurrently \"npm run watch:widget:lib\" \"npm run start:widget:browser\" \"npm run start:widget:server\"",
    "build:widget:lib": "cd modules/widget && node build.js run",
    "build:widget:app": "cd modules/widget && webpack",
    "build:widget": "npm run build:widget:lib && npm run build:widget:app",
    "watch:widget:lib": "gulp watch:lib",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },

